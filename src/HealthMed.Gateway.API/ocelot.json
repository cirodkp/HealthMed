{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/pacientes/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "paciente-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/pacientes/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/medicos/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "medico-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/medicos/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/agenda/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "agenda-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/agenda/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/consultas/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "consulta-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/consultas/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "auth-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}

